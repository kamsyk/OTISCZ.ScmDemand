<Window x:Class="OTISCZ.ScmDemand.UI.ScmWindow.WinNomenclature"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OTISCZ.ScmDemand.UI.ScmWindow"
        xmlns:localView="clr-namespace:OTISCZ.ScmDemand.UI.View"
        mc:Ignorable="d"
        Title="Nomenclature" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>


        <DockPanel x:Name="dpPlaceholder"  Grid.Row="0">
            <localView:NomenclatureDetail x:Name="ucNomenclatureDetail" />
        </DockPanel>
    </Grid>
</Window>
